{
  "expo": {
    "name": "mobile",
    "slug": "instedu",
    "owner": "orikfw",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/instedu_logo_black.png",
    "userInterfaceStyle": "light",
    "scheme": "light",
    "splash": {
      "image": "./assets/img/instedu_logo_black.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.orikfw.instedu"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/instedu_logo_black.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/img/instedu_logo_black.png"
    }
  }
}
